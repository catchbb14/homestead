<section class="block">
  <div class="container">
    <h2>Classifides</h2>
    <ul class="categories-list clearfix">
      {{#each hbsObject.classifieds}}
        <li>
          <i class="fa {{this.icon}} fa-2x"></i>
          <h3><a href="{{this.url}}">{{this.title}}</a></h3>
          <div class="sub-categories">
            {{#each this.subcats}}
              <a href="{{this.url}}">{{this.title}}</a>
            {{/each}}
          </div>
        </li>
      {{/each}}
    </ul>
  </div>
</section>
